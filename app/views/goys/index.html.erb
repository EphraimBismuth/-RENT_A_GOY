<div class="title">
  <div class="title-text">
    <h3 class="text-center">See the available Goys in Tel-aviv</h3></div>
  <div class="title-btn text-center">
    <%= link_to "edit search", root_path, class: "btn-gag"%>
  </div>
</div>

<% content_for :below_container do %>
<div class="main-container">
  <div class="index-goys">
    <div class="row">

      <% @goys.each do |goy| %>
        <%= render 'components/card', goy: goy %>
      <% end %>

    </div>
  </div>

    <div
    id="map"
    style="width: 100%;"
    data-markers="<%= @markers.to_json %>"
  ></div>
</div>

<% end %>

